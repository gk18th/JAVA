//Paper Copier//
public class Main {
  public static void main(String[] args) {
    int paper = 1;
    for (paper=1;paper<=10;paper++) 
    {
      System.out.println("Paper" + " " + paper);
    }  
  }
}

Paper 1
Paper 2
Paper 3
Paper 4
Paper 5
Paper 6
Paper 7
Paper 8
Paper 9
Paper 10
