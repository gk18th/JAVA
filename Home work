// Book Paper Copier//
public class Main {
  public static void main(String[] args) {
    int Book = 1;
    for (Book = 1; Book <= 3; Book++) 
    {
    int paper = 1;
    for (paper = 1; paper <= 3; paper++) 
    {
      System.out.println( "Book" + " " + Book + "Paper" + " " + paper);
    }  
  }
}
}

Book 1Paper 1
Book 1Paper 2
Book 1Paper 3
Book 2Paper 1
Book 2Paper 2
Book 2Paper 3
Book 3Paper 1
Book 3Paper 2
Book 3Paper 3
.........................................................................


// Bucket//
public class Main {
  public static void main(String[] args) {
    int Orange = 1;
    while (Orange <= 50)
    {   
      System.out.println( "Orange" + " " + Orange); 
      if(Orange ==10)
      {
      System.out.println( "Bucket is full");
      break;
        }
      Orange++ ;
    }  
  }
}


Orange 1
Orange 2
Orange 3
Orange 4
Orange 5
Orange 6
Orange 7
Orange 8
Orange 9
Orange 10
Bucket is full
.........................................................................


//Cars//
public class Main {
  public static void main(String[] args) {
    String[] cars = {"Volvo", "BMW", "Ford", "Mazda", "Tata","Maruthi",};
    for (String i : cars) {
      System.out.println(i);
    }    
  }
}


